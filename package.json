{
  "name": "@casd/print-tree",
  "version": "0.0.3",
  "description": "Create plain text tree, like the unix `tree` command",
  "author": "Cas Dijkman <info@cdijkman.nl> (https://cdijkman.nl)",
  "keywords": [
    "print",
    "tree",
    "unix tree"
  ],
  "homepage": "https://cdijkman.nl/",
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/casdijkman/print-tree.git"
  },
  "main": "dist/print-tree.js",
  "types": "index.d.ts",
  "packageManager": "yarn@4.6.0",
  "devDependencies": {
    "@types/xo": "^0.39.9",
    "typescript": "^5.8.3",
    "xo": "^0.60.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "yarn lint",
    "lint": "xo --space"
  },
  "bugs": {
    "url": "https://github.com/casdijkman/print-tree/issues"
  },
  "xo": {
    "ignores": [
      "dist"
    ],
    "rules": {
      "@typescript-eslint/object-curly-spacing": [
        "error",
        "always"
      ],
      "arrow-parens": [
        "error",
        "always"
      ]
    }
  }
}
